{
  "name": "backend",
  "version": "1.0.0",
  "description": "The backend for our app",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist",
    "dev": "ts-node-dev --rs --no-notify --respawn --transpile-only src",
    "prisma": "prisma format && prisma generate",
    "postinstall": "npm run prisma && docker compose up --detach && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CroudsourcedCombos/backend.git"
  },
  "dependencies": {
    "@prisma/client": "~3.3.0",
    "@types/graphql-fields": "^1.3.4",
    "apollo-server": "^3.4.0",
    "class-validator": "^0.13.1",
    "graphql": "^15.6.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.12.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "10.4.0",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "0.16.3",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@types/node": "12.20.36",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "2.4.1",
    "ts-node-dev": "1.1.8",
    "prisma": "~3.3.0"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CroudsourcedCombos/backend/issues"
  },
  "homepage": "https://github.com/CroudsourcedCombos/backend#readme",
  "prettier": {
    "semi": false,
    "useTabs": false,
    "tabWidth": 2,
    "printWidth": 80
  }
}
